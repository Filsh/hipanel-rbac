            "homepage": "http://hiqdev.com/"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2": "dev-master",
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hipanel\\rbac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hipanel\\rbac\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "common": "src/config/common.php",
            "console": [
                "src/config/common.php",
                "src/config/console.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
